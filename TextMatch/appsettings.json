{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HttpsPort":443,
  "Hsts":{
    "IsHstsEnabled":true,
    "MaxAgeByDay":90,
    "IncludeSubDomains":true,
    "ExcludedDomains":["real-domain-example.com", "www.real-domain-example.com"],
    "IsPreload":true
  },
  "AllowedHosts": "*",
  "AllowedOrigins":["realdomain.com"]
}
